{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "sticky-layout#desktop"
    ]
  },
  "sticky-layout#desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": [
      "flex-layout.row#desktop",
      "flex-layout.row#desktop2"
    ]
  },
  "flex-layout.row#desktop": {
    "props": {
      "blockClass": "header-left"
    },
    "children": [
      "flex-layout.col#logo-desktop"
    ]
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": [
      "logo"
    ]
  },
  "logo": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "data:image/png;base64,",
      "width": "102"
    }
  },
  "flex-layout.row#desktop2": {
    "props": {
      "colSizing": "auto",
      "blockClass": "header-right"
    },
    "children": [
      "search-bar",
      "link.product#product-page",
      "modal-trigger#mis-productos",
      "minicart.v2#op2",
      "btn-sony",
      "minicart.v2"
    ]
  },
  "search-bar": {
    "props": {
      "placeholder": "Buscar productos por nombre, código o categoría..."
    }
  },
  "flex-layout.col#spacer": {
    "props": {
      "width": "grow"
    }
  },
  "header-layout.mobile": {
    "children": [
      "sticky-layout#mobile"
    ]
  },
  "sticky-layout#mobile": {
    "children": [
      "flex-layout.row#mobile"
    ]
  },
  "flex-layout.row#mobile": {
    "children": [
      "drawer",
      "logo",
      "search-bar",
      "flex-layout.col#spacer",
      "link.product#product-page",
      "minicart.v2#op2",
      "minicart.v2"
    ],
    "props": {
      "blockClass": "main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "drawer": {
    "children": []
  },
  "link.product#product-page": {
    "props": {
      "href": "/centro-de-ayuda",
      "label": "AYUDA",
      "blockClass": "link-centro-ayuda"
    }
  },
  "modal-trigger#mis-productos": {
    "children": [
      "rich-text#mis-productos",
      "modal-layout#mis-productos"
    ]
  },
  "rich-text#mis-productos": {
    "props": {
      "text": "MIS PEDIDOS",
      "blockClass": "mis-pedidos"
    }
  },
  "modal-layout#mis-productos": {
    "children": [
      "login-content"
    ]
  },
  "login": {
    "props": {
      "iconLabel": "Mis pedidos",
      "emailAndPasswordTitle": "LOG-IN",
      "accessCodeTitle": "Acess Code LOG-IN",
      "emailPlaceholder": "e-mail",
      "passwordPlaceholder": "password",
      "showPasswordVerificationIntoTooltip": true,
      "blockClass": "login-btn"
    }
  },
  "minicart.v2#op2": {
    "props": {
      "variation": "popup",
      "openOnHover": true,
      "itemCountMode": "total",
      "MinicartIcon": "btn-dots",
      "quantityDisplay": "never"
    },
    "children": [
      "dots-menu"
    ]
  },
  "flex-layout.col#dots": {
    "children": [
      "flex-layout.row#info",
      "link#dots-info2",
      "link#dots-info3"
    ]
  },
  "flex-layout.row#info": {
    "props": {
      "colSizing": "auto"
    },
    "children": [
      "flex-layout.row#info2",
      "icon-next"
    ]
  },
  "flex-layout.row#info2": {
    "props": {
      "colSizing": "auto"
    },
    "children": [
      "icon-quest",
      "link#dots-info"
    ]
  },
  "link#dots-info": {
    "props": {
      "label": "Ayuda",
      "blockClass": "link"
    }
  },
  "link#dots-info2": {
    "props": {
      "label": "Login Sony",
      "blockClass": "link"
    }
  },
  "link#dots-info3": {
    "props": {
      "label": "Sitio Sony",
      "blockClass": "link"
    }
  }
}